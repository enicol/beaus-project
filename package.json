{
  "name": "beaus",
  "version": "1.0.0",
  "description": "Beaus All Natural app",
  "author": {
    "name": "Emma Nicol"
  },
  "homepage": "https://enicol.github.io/beaus-project/",
  "license": "MIT",
  "scripts": {
